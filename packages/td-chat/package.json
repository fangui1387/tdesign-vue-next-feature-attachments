{
  "name": "@jump-mp/td-chat",
  "version": "1.0.0",
  "description": "TDesign Chat Component for Vue 2.7",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/tdesign-vue-chat.min.js",
  "jsdelivr": "dist/tdesign-vue-chat.min.js",
  "typings": "lib/index.d.ts",
  "files": [
    "es",
    "lib",
    "dist",
    "README.md",
    "global.d.ts",
    "docs",
    "examples"
  ],
  "scripts": {
    "build": "npm run build:es && npm run build:cjs && npm run build:umd && npm run build:css",
    "build:es": "rollup --config rollup.config.js --format es --dir es --bundleConfigAsCjs",
    "build:cjs": "rollup --config rollup.config.js --format cjs --dir lib --bundleConfigAsCjs",
    "build:umd": "rollup --config rollup.config.js --format umd --dir dist --bundleConfigAsCjs",
    "build:css": "echo 'CSS styles are imported from tdesign-vue'",
    "dev": "rollup --config rollup.config.js --format es --dir es --watch --bundleConfigAsCjs",
    "test": "pnpm run build && node ./test/run-jest.js",
    "check:vue2": "node scripts/vue2-compat-check.mjs"
  },
  "keywords": [
    "tdesign",
    "chat",
    "vue",
    "vue2",
    "component"
  ],
  "author": "tdesign",
  "license": "MIT",
  "peerDependencies": {
    "tdesign-vue": "^1.0.0",
    "vue": "^2.7.0"
  },
  "dependencies": {
    "clipboard": "^2.0.11",
    "highlight.js": "^11.9.0",
    "lodash-es": "^4.17.21",
    "marked": "^12.0.0",
    "marked-highlight": "^2.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "29",
    "@types/lodash-es": "^4.17.12",
    "@vue/test-utils": "^1.3.6",
    "@vue/vue2-jest": "29",
    "jest": "29",
    "jest-environment-jsdom": "29",
    "lodash": "^4.17.23",
    "rollup": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-vue": "5.1.9",
    "tdesign-icons-vue": "^0.2.0",
    "tdesign-vue": "^1.10.0",
    "ts-jest": "29",
    "typescript": "^5.0.0",
    "vue": "2.7.16",
    "vue-template-compiler": "2.7.16"
  }
}
